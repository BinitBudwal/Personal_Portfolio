  },
  "homepage": "https://github.com/BinitBudwal/Personal_Portfolio#readme",
  "devDependencies": {
    "clean-css-cli": "^5.5.2",
    "imagemin-cli": "^7.0.0",
    "uglify-js": "^3.15.3"
  },
  "dependencies": {
    "cp": "^0.2.0",
    "rollup": "^2.70.0"
  }
}
