{
  "name": "personal_portfolio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:css": "cleancss src/css/*.css -o dist/css/style.min.css",
    "build:js": "mkdir -p dist/js && touch dist/js/main.js && cat src/js/*.js >> dist/js/main.js",
    "copy:html": "cp scr/*.html dist && cp src/pages/*.html dist",
    "imagemin": "imagemin src/images/* -o dist/images",
    "rollup": "rollup src/js/*.js --file dist/js/main.js --format es" ,
    "build": "npm run build:css && npm run copy:html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BinitBudwal/Personal_Portfolio.git"
  },
